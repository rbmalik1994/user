<h3> Users </h3>
<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Email</th>
    <th>Password</th>
    <th></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
  <% _.each(users, function(user) { %>
    <tr  data-id="<%= user.id %>" data-model="user" >
      <td><%= user.id %></td>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.password %></td>
      <td><a href="/user/show/<%= user.id %>">Show</a></td>
      <td><a href="/user/edit/<%= user.id %>">Edit</a></td>
      <td><a href="/user/destroy/<%= user.id %>">Delete</a></td>
    </tr>
    <% }) %>
  </table>
