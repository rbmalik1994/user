<div class="container">
  <div class="row">
    <div class="col-sm-4">
      <h1> Dashboard!</h1>

      <a href="/user/login">login</a> &nbsp;| &nbsp;
      <a href="/user/new"> Signup</a>
    </div>
    <div class="col-sm-8">
      loged in user
      <h3> <%= user.name %> </h3>
      <h3> Email: <%= user.email %></h3>
      <a href="/user/logout/"> logout</a>
    </div>

      <hr>
  </div>
<h3> Users </h3>
<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Email</th>
    <th>Password</th>
    <th></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
  <% _.each(users, function(user) { %>
    <tr  data-id="<%= user.id %>" data-model="user" >
      <td><%= user.id %></td>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.password %></td>
      <td><a href="/user/show/<%= user.id %>">Show</a></td>
      <td><a href="/user/edit/<%= user.id %>">Edit</a></td>
      <td><a href="/user/destroy/<%= user.id %>">Delete</a></td>
    </tr>
    <% }) %>
  </table>
</div>
